E6BEG[0|0|0|0|0|0|0|0],[o_e2w0|o_e2w1|o_e2w2|o_e2w3|o_e2w4|o_e2w5|o_e2w6|o_e2w7]
E6BEG[1|1|1|1|1|1|1|1],[o_e2w0|o_e2w1|o_e2w2|o_e2w3|o_e2w4|o_e2w5|o_e2w6|o_e2w7]
E6BEG[0|0|0|0|0|0|0|0],[J2MID_ABb_END1|J2MID_CDb_END1|J2MID_EFb_END1|J2MID_GHb_END1|E1END3|W1END3|NN4END0|SS4END0]
E6BEG[1|1|1|1|1|1|1|1],[J2MID_ABa_END2|J2MID_CDa_END2|J2MID_EFa_END2|J2MID_GHa_END2|E1END2|W1END2|NN4END3|SS4END3]

W6BEG[0|0|0|0|0|0|0|0],[o_e2w0|o_e2w1|o_e2w2|o_e2w3|o_e2w4|o_e2w5|o_e2w6|o_e2w7]
W6BEG[1|1|1|1|1|1|1|1],[o_e2w0|o_e2w1|o_e2w2|o_e2w3|o_e2w4|o_e2w5|o_e2w6|o_e2w7]
